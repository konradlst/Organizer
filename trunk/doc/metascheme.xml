<!DOCTYPE cg>
<cg_db_scheme version="0.1">
    <tables>
        <table name="CG_FINANCEACCOUNTS">
            <field name="id" type="INTEGER" pk="1" delegate="id"/>
            <field name="number" type="VARCHAR(20)" unq="1" delegate="int20"/>
            <field name="status" type="BOOLEAN" nullable="0" default="'0'" delegate="checkBox"/>
            <field name="type" type="TINYINT" nullable="0" default="'0'" delegate="typeAccount"/>
            <field name="name" type="VARCHAR(100)" unq="1" nullable="0" default="' '" delegate="string100"/>
            <field name="value" type="INTEGER" delegate="int"/>
        </table>
        <table name="CG_FINANCELOG">
            <field name="id" type="INTEGER" pk="1" delegate="id"/>
            <field name="timestamp" type="TIMESTAMP" unq="1" default="(datetime('now','localtime'))" delegate="dateTime"/>
            <field name="operation" type="VARCHAR(100)" delegate="string100"/>
            <field name="value" type="INTEGER" nullable="0" default="'0'"/>
            <field name="sendfrom" type="VARCHAR(20)"/>
            <field name="sendto" type="VARCHAR(20)"/>
            <field name="comment" type="VARCHAR(255)"/>
        </table>
        <table name="CG_TIME">
            <field name="id" type="INTEGER" pk="1" delegate="id"/>
            <field name="created" type="DATE" delegate="date"/>
            <field name="type" type="VARCHAR(30)"/>
            <field name="duration" type="TIME" delegate="time"/>
        </table>
        <table name="CG_BOOK">
            <field name="id" type="INTEGER" pk="1" delegate="id"/>
            <field name="lastUpdate" type="DATE" delegate="date"/>
            <field name="author" type="VARCHAR(50)"/>
            <field name="name" type="VARCHAR(50)"/>
            <field name="section" type="VARCHAR(50)"/>
            <field name="format" type="VARCHAR(50)"/>
            <field name="pages" type="INTEGER" delegate="int"/>
            <field name="year" type="INTEGER" delegate="int"/>
            <field name="ISBN" type="VARCHAR(20)"/>
            <field name="publishing" type="VARCHAR(50)"/>
            <field name="count" type="INTEGER" delegate="int"/>
        </table>
        <table name="CG_SPORT">
            <field name="id" type="INTEGER" pk="1" delegate="id"/>
            <field name="created" type="DATE" delegate="date"/>
            <field name="type" type="VARCHAR(30)"/>
            <field name="value" type="INTEGER" delegate="int"/>
            <field name="duration" type="TIME" delegate="time"/>
        </table>
        <table name="CG_DEAL">
            <field name="id" type="INTEGER" pk="1" delegate="id"/>
            <field name="created" type="DATE" delegate="date"/>
            <field name="deadline" type="DATE" delegate="date"/>
            <field name="description" type="VARCHAR(127)" delegate="string127"/>
            <field name="price" type="INTEGER" delegate="int"/>
            <field name="duration" type="TIME" delegate="time"/>
        </table>
        <table name="CG_CONTACTS">
            <field name="id" type="INTEGER" pk="1" delegate="id"/>
            <field name="alias" type="VARCHAR(255)" unq="1" nullable="0" default="'alias'"/>
            <field name="name" type="VARCHAR(100)" delegate="string100"/>
            <field name="lastName" type="VARCHAR(100)" delegate="string100"/>
            <field name="otherName" type="VARCHAR(100)" delegate="string100"/>
            <field name="birthday" type="DATE" delegate="date"/>
            <field name="pathToUserPic" type="VARCHAR(255)"/>
            <field name="comment" type="VARCHAR(255)"/>
        </table>
        <table name="CG_COMPANIES">
            <field name="id" type="INTEGER" pk="1" delegate="id"/>
            <field name="name" type="VARCHAR(100)" unq="1" delegate="string100"/>
            <field name="phone" type="VARCHAR(100)" delegate="string100"/>
            <field name="department" type="VARCHAR(127)" delegate="string127"/>
            <field name="post" type="VARCHAR(100)" delegate="string100"/>
            <field name="address" type="VARCHAR(255)"/>
            <field name="dateIn" type="DATE" delegate="date"/>
            <field name="dateOut" type="DATE" delegate="date"/>
            <field name="ownerId" type="INTEGER" delegate="int"/>
        </table>
        <table name="CG_ADDRESSES">
            <field name="id" type="INTEGER" pk="1" delegate="id"/>
            <field name="country" type="VARCHAR(127)" delegate="string127"/>
            <field name="city" type="VARCHAR(127)" delegate="string127"/>
            <field name="street" type="VARCHAR(127)" delegate="string127"/>
            <field name="home" type="VARCHAR(127)" delegate="string127"/>
            <field name="apartment" type="VARCHAR(127)" delegate="string127"/>
            <field name="type" type="VARCHAR(100)" delegate="string100"/>
            <field name="ownerId" type="INTEGER" delegate="int"/>
        </table>
        <table name="CG_CHANNELS">
            <field name="id" type="INTEGER" pk="1" delegate="id"/>
            <field name="format" type="VARCHAR(100)" nullable="0" default="'Phone'" delegate="string100"/>
            <field name="title" type="VARCHAR(100)" delegate="string100"/>
            <field name="value" type="VARCHAR(100)" delegate="string100"/>
            <field name="type" type="VARCHAR(100)" delegate="string100"/>
            <field name="ownerId" type="INTEGER" delegate="int"/>
        </table>
    </tables>
    <values>
        <table name="CG_FINANCEACCOUNTS">
            <field name="number" value="1234567890"/>
            <field name="status" value="1"/>
            <field name="type" value="3"/>
            <field name="name" value="bank debet"/>
            <field name="value" value="5000"/>
        </table>
        <table name="CG_FINANCELOG">
            <field name="timestamp" value="2000.01.01 00:00:00"/>
            <field name="operation" value="1"/>
            <field name="value" value="3000"/>
            <field name="sendfrom" value="vtb debet"/>
            <field name="sendto" value="rsb card"/>
            <field name="comment" value="test"/>
        </table>
        <table name="CG_TIME">
            <field name="created" value="2000.01.01"/>
            <field name="type" value="sleep"/>
            <field name="duration" value="10:00"/>
        </table>
    </values>
    <delegates>
        <delegate name="id" type="lineEdit" param1="8" param2="" param3="true">
        <delegate name="int20" type="lineEdit" param1="20" param2="00000000000000000000" param3="false">
        <delegate name="checkBox" type="checkBoxEdit"/>
        <delegate name="typeAccount" type="comboBoxEdit" param1="cash;card;deposit;IN;OUT;debet;credit;account" param2="false"/>
        <delegate name="int" type="lineEdit" param1="8" param2="" param3="false">
        <delegate name="string100" type="lineEdit" param1="100" param2="" param3="false">
        <delegate name="string127" type="lineEdit" param1="127" param2="" param3="false">
        <delegate name="checkBox" type="chechBoxEdit"/>
        <delegate name="time" type="timeEdit"/>
        <delegate name="date" type="dateEdit"/>
        <delegate name="dateTime" type="dateTimeEdit"/>
    </delegates>
</cg_db_scheme>
